{
  "name": "rollup-buble-mocha-boilerplate",
  "version": "1.0.1",
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "description": "Rollup + Buble + Mocha",
  "main": "dist/lib.umd.js",
  "module": "dist/lib.es6.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/w8r/rollup-buble-mocha-boilerplate"
  },
  "bugs": "https://github.com/w8r/rollup-buble-mocha-boilerplate/issues",
  "keywords": [
    "rollup",
    "buble",
    "mocha"
  ],
  "scripts": {
    "prebuild": "eslint src tests/",
    "build": "rollup -c rollup.config.umd.js && rollup -c rollup.config.es6.js",
    "pretest": "rollup -c rollup.config.tests.js",
    "start": "npm run test:watch",
    "test:watch": "nodemon --watch src --watch tests --exec 'npm test'",
    "test": "mocha build/tests-bundle.js",
    "test:browser": "npm run build && npm run pretest && opn http://127.0.0.1:8080/tests/ && static -p 8080",
    "prepare": "npm run build && npm test"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "node-static": "0.7.10",
    "nodemon": "^1.11.0",
    "opn-cli": "3.1.0",
    "rollup": "^0.52.2",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "source-map-support": "^0.5.0"
  },
  "dependencies": {}
}
